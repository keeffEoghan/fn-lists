{
  "name": "@epok.tech/fn-lists",
  "version": "0.1.0",
  "description": "Simple utils around JS lists (array-like) for flexible and terse code - new arrays by default (for FP/immutable), mutate if specified (to reuse memory).",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/keeffEoghan/fn-lists.git"
  },
  "author": "Eoghan O'Keeffe <epok.tech@gmail.com> (https://epok.tech)",
  "license": "MIT",
  "engines": {
    "node": "v16.13.0 || *",
    "yarn": "3.1.0 || *"
  },
  "packageManager": "yarn@3.1.0",
  "scripts": {
    "start": "yarn build",
    "dev": "parcel --log-level verbose --target es --target cjs",
    "build": "rm -rf ./dist/ && parcel build --target es --target cjs",
    "build:safe": "yarn build --no-cache"
  },
  "devDependencies": {
    "parcel": "^2.0.1"
  },
  "main": "dist/main/index.js",
  "module": "dist/es/index.js",
  "source": [
    "index.js",
    "each.js",
    "iterable.js",
    "map.js",
    "range.js",
    "reduce.js",
    "wrap-index.js"
  ],
  "files": [
    "dist/**/*"
  ],
  "targets": {
    "cjs": {
      "outputFormat": "commonjs",
      "isLibrary": true,
      "engines": {
        "browsers": "last 2 versions, > 0.5%, not dead"
      },
      "distDir": "dist/main/",
      "publicUrl": "dist/main/"
    },
    "es": {
      "outputFormat": "esmodule",
      "isLibrary": true,
      "engines": {
        "browsers": "last 2 versions, > 0.5%, not dead"
      },
      "distDir": "dist/es/",
      "publicUrl": "dist/es/"
    }
  },
  "bugs": {
    "url": "https://github.com/keeffEoghan/fn-lists/issues"
  },
  "homepage": "https://github.com/keeffEoghan/fn-lists#readme",
  "keywords": [
    "array",
    "utils",
    "functional",
    "fp"
  ]
}
